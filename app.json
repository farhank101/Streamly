  "expo": {
    "scheme": "streamly",
    "web": {
      "bundler": "metro",
      "headers": {
        "Cross-Origin-Opener-Policy": "same-origin",
        "Cross-Origin-Embedder-Policy": "require-corp"
      }
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-speech-recognition",
      "expo-font"
    ],
    "name": "Streamly",
    "slug": "Streamly"
  }
}
